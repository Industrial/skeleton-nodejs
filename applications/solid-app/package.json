{
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.3",
    "@solidjs/start": "^1.0.6",
    "solid-js": "^1.8.21",
    "vinxi": "^0.4.1"
  },
  "devDependencies": {
    "@rollup/rollup-linux-x64-musl": "^4.21.0",
    "npm-check-updates": "^17.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "name": "example-basic",
  "scripts": {
    "build": "bun run build:haskell && bun run build:vinxi",
    "build:haskell": "cd src/haskell && wasm32-wasi-ghc lib/MyLib.hs -o dist/MyLib.wasm -no-hs-main -optl-mexec-model=reactor -optl-Wl,--export=hs_init,--export=fib",
    "build:vinxi": "vinxi build",
    "dev": "vinxi dev",
    "lint": "tsc -p . --noEmit && eslint --fix .",
    "start": "vinxi start",
    "update-packages": "bun npm-check-updates -u",
    "version": "vinxi version"
  },
  "type": "module"
}