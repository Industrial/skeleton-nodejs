{
  "name": "@code9/permutation-generator",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf .spago dist output",
    "deps": "spago install",
    "build": "spago bundle-app --to dist/mod.js --platform=node",
