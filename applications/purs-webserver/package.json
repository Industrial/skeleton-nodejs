{
  "name": "@code9/purs-webserver",
  "module": "dist/mod.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf .spago dist output",
    "deps": "spago install",
    "build": "spago bundle-app --to dist/mod.js --platform=node",
    "build:watch": "bun run build --watch",
    "start": "bun run dist/mod.js",
    "start:watch": "bun run --watch dist/mod.js"
  },
  "devDependencies": {
    "bun-types": "^0.5.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "purescript": "^0.15.15",
    "spago": "^0.21.0"
  }
}