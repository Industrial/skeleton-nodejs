{
  "name": "@code9/test-onnx",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "applications/test-onnx/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "platform": "node",
        "outputPath": "dist/test-onnx",
        "format": [
          "esm"
        ],
        "bundle": false,
        "main": "applications/test-onnx/src/main.ts",
        "tsConfig": "applications/test-onnx/tsconfig.json",
        "assets": [
          "applications/test-onnx/src/assets"
        ],
        "generatePackageJson": false,
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": true,
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            }
          }
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "test-onnx:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "test-onnx:build:development"
        },
        "production": {
          "buildTarget": "test-onnx:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "../../coverage/packages/test-onnx"
      }
    }
  },
  "tags": []
}