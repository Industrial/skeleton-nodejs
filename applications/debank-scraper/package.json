{
  "name": "@code9/debank-scraper",
  "type": "module",
  "scripts": {
    "build": "vinxi build",
    "db:seed": "bash -c 'bun run src/lib/db/seed.ts'",
    "db:generate": "bash -c 'bunx npm-run-all db:generate:*'",
    "db:generate:queries": "bash -c 'bunx @edgedb/generate queries --file src/lib/db/generated/queries --force-overwrite'",
    "db:generate:client": "bash -c 'bunx @edgedb/generate edgeql-js --out src/lib/db/generated/client --force-overwrite'",
    "db:generate:interfaces": "bash -c 'bunx @edgedb/generate interfaces --file src/lib/db/generated/interfaces.ts --force-overwrite'",
    "db:migrate": "edgedb migrate",
    "db:migration:create": "edgedb migration create",
    "db:ui": "edgedb ui",
    "db:watch": "edgedb watch",
    "dev": "vinxi dev",
    "lint": "tsc -p . --noEmit && eslint --fix .",
    "start": "vinxi start",
    "version": "vinxi version"
  },
  "dependencies": {
    "@edgedb/generate": "^0.5.3",
    "@solidjs/meta": "^0.29.3",
    "@solidjs/router": "^0.13.3",
    "@solidjs/start": "^1.0.0-rc.0",
    "edgedb": "^1.5.7",
    "solid-js": "^1.8.17",
    "vinxi": "^0.3.11"
  },
  "engines": {
    "node": ">=18"
  }
}
